{
	"name": "SectionRatings",
	"author": "Xen alpha(Senouis)",
	"url": "https://libertyga.me",
	"description": "This extension modifies and sorts result of database table used by RatePage extension.",
	"version": "0.1",
	"license-name": "GPL-2.0-or-later",
	"type": "others",
	"AutoloadNamespaces":{
		"Mediawiki\\Extension\\SectionRatings\\": "includes/"
	},
	"RestRoutes": [
		{
			"path": "/sectionratings/v0/rategame/{gamename}/{score}",
			"method": "POST",
			"class": "Mediawiki\\Extension\\SectionRatings\\RateGame"
		}
		
	],
	"config":{
		"SeRaTargetContest": [],
		"SeRaTopNumber": 5
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.35.0",
		"VoteNY": ">= 3.2"
	}
}