{
  "name": "SectionRatings",
  "author": "Xen alpha(Senouis)",
  "url": "https://libertyga.me",
  "description": "This extension modifies result of database table used by VoteNY extension.",
  "version": "0.5.0",
  "license-name": "MIT",
  "type": "others",
  "AutoloadNamespaces": {
    "MediaWiki\\Extension\\SectionRatings\\": "src/",
    "MediaWiki\\Extension\\SectionRatings\\Utils\\" : "includes/"
  },
  "RestRoutes": [
    {
      "path": "/sectionratings/v0/ratings/{category}/{count}",
      "method": "GET",
      "class": "MediaWiki\\Extension\\SectionRatings\\GetGameRatings"
    },
    {
      "path": "/sectionratings/v0/categorycounter/{category}/{namespace}",
      "method": "GET",
      "class": "MediaWiki\\Extension\\SectionRatings\\CategoryCounter"
    },
    {
      "path": "/sectionratings/v1/ratings/{category}/{count}",
      "method": "GET",
      "class": "MediaWiki\\Extension\\SectionRatings\\GetGameRatings"
    },
    {
      "path": "/sectionratings/v1/categorycounter/{category}/{namespace}",
      "method": "GET",
      "class": "MediaWiki\\Extension\\SectionRatings\\CategoryCounter"
    },
    {
      "path": "/sectionratings/v1/userratings/{user}",
      "method": "GET",
      "class": "MediaWiki\\Extension\\SectionRatings\\Utils\\UserRatings"
    }
  ],
  "manifest_version": 2,
  "requires": {
    "MediaWiki": ">= 1.37.0",
    "extensions": {
      "VoteNY": "*"
    }
  }
}
