{
	"name": "SectionRatings",
	"author": "Xen alpha(Senouis)",
	"url": "https://libertyga.me",
	"description": "This extension modifies result of database table used by VoteNY extension.",
	"version": "0.1",
	"license-name": "GPL-2.0-or-later",
	"type": "others",
	"AutoloadNamespaces":{
		"Mediawiki\\Extension\\SectionRatings\\": "includes/"
	},
	"RestRoutes": [
		{
			"path": "/sectionratings/v0/rategame/{gamename}/{score}",
			"method": "GET",
			"class": "Mediawiki\\Extension\\SectionRatings\\RateGame"
		}
		
	],
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.37.0",
		"extensions" : {
			"VoteNY": "*"
		}
	}
}