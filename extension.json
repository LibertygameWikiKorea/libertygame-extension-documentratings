{
	"name": "SectionRatings",
	"author": "Xen alpha(Senouis)",
	"url": "https://libertyga.me",
	"description": "This extension modifies and sorts result of database table used by RatePage extension.",
	"version": "0.1",
	"license-name": "GPL-2.0-or-later",
	"type": "others",
	"AutoloadClasses":{
		"RateDB": "includes/RateDB.php"
	},
	"callback": "SectionRatings\\RateDB::onExtensionLoad",
	"Hooks": {
		"LoadExtensionSchemaUpdates": "SectionRatings\\RateDB::onLoadExtensionSchemaUpdates",
		"ParserFirstCallInit": "SectionRatings\\RateDB::onParserFirstCallInit",
		"RateCreated": "SectionRatings\\RateDB::onRateCreated",
		"RateImported": "SectionRatings\\RateDB::onRateImported",
		"RateResultRequested": "SectionRatings\\RateDB::onRateResultRequested"
	},
	"config":{
		"SeRaTargetContest": [],
		"SeRaTopNumber": 5
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.36.0",
		"RatePage": ">= 1.0.0"
	}
}